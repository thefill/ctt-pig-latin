    "scripts": {
        "start": "yarn ng serve",
        "build": "yarn clean && yarn ng build",
        "build-prod": "yarn ng build --env=prod",
        "test": "yarn ng test --code-coverage",
        "test-once": "yarn test --single-run",
        "lint": "yarn ng lint",
        "e2e": "yarn ng e2e",
        "clean": "rimraf dist",
        "lint-test-build": "yarn lint && yarn test-once && yarn build",
        "git-add": "git add . && git add coverage dist --force",
        "git-commit": "git commit -m \"Pre-push build\"",
        "precommit": "yarn lint",
        "prepush": "yarn lint-test-build && yarn git-add && git-commit"
    },
    "private": true,
    "dependencies": {
